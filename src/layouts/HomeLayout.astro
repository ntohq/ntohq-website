---
import BaseLayout from '@layouts/BaseLayout.astro'
import Navbar from '@components/Navbar.vue';
import NavFooter from '@components/NavFooter.astro';

export interface Props {
	title: string;
	contentType: string;
	description: string;
	image: string;
}
const { 
	title,
	contentType = "website",
    description,
    image
 } = Astro.props as Props;
---

<BaseLayout {title} {contentType} {description} {image}>
	<Navbar client:load></Navbar>
	<main class="flex-1 flex flex-col">
		<slot />
	</main>
	<NavFooter></NavFooter>
</BaseLayout>